    <script src='/wp-content/themes/artpaint/js/jquery.min68b3.js' id='jquery-js'></script>
    <script src='/wp-content/themes/artpaint/js/fancybox.min68b3.js' id='fancybox-js'></script>
    <script src='/wp-content/themes/artpaint/js/swiper.min68b3.js' id='swiper-js'></script>
    <script src='/wp-content/themes/artpaint/js/odometer.min68b3.js' id='odometer-js'></script>
    <script src='/wp-content/themes/artpaint/js/wow.min68b3.js' id='wow-js'></script>
