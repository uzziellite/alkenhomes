    <div class="preloader">
      <figure>
        <img src="/images/logo.png" alt="Image">
      </figure>
    </div>
    <div class="page-transition"></div>
    <Header />
      <slot />
    <Footer />
    <script src='/wp-includes/js/dist/vendor/regenerator-runtime.min3937.js' id='regenerator-runtime-js'></script>
    <script src='/wp-includes/js/dist/vendor/wp-polyfill.min2c7c.js' id='wp-polyfill-js'></script>
